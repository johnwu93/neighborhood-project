  "description": "Configuration settings for writing Front End Development Code for Udacity Projects",
  "main": "gulpfile.js",
  "scripts": {
    "clean:dev": "rm -rf temp/",
    "clean:prod": "rm -rf prod/",
    "build:dev": "npm run clean:dev && node_modules/.bin/webpack --config tasks/webpack/webpack.dev.js",
    "build:prod": "npm run clean:prod && node_modules/.bin/webpack --config tasks/webpack/webpack.prod.js",
    "start": "node_modules/.bin/webpack-dev-server --config tasks/webpack/webpack.dev.js --open",
    "deploy": "sh tasks/heroku-deploy.sh"
